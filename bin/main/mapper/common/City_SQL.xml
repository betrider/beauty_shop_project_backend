<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shop.beauty.api.common.mapper.CityMapper">
    <select id="selectAllCity" resultType="com.shop.beauty.api.common.model.CityModel">
        SELECT
            name,
            population,
            altitude
        FROM
            city
    </select>
</mapper>